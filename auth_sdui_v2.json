      "onboarding_step1_contact": {
        "id": "onboarding_step1_contact",
        "type": "form",
        "theme": "dark",
        "header": {
          "title": {
            "text": "How should we verify you?",
            "family": "Roboto-Bold",
            "size": {
              "phone": 24,
              "tablet": 48
            },
            "weight": "700",
            "style": "normal",
            "color": {
              "light": "#1B1D26",
              "dark": "#ffffff"
            }
          },
          "secondaryTitle": {
            "text": "You can verify your account using either your email address or your mobile number.",
            "family": "Roboto-Regular",
            "size": {
              "phone": 14,
              "tablet": 20
            },
            "weight": "400",
            "style": "normal",
            "color": {
              "light": "#6B7280",
              "dark": "#98989F"
            }
          }
        },
        "fields": [
          {
            "id": "contact_method_toggle",
            "type": "toggle",
            "label": "Preferred contact method",
            "options": [
              {
                "id": "email",
                "label": "Email"
              },
              {
                "id": "mobile",
                "label": "Mobile Number"
              }
            ]
          },
          {
            "id": "email",
            "type": "text",
            "label": "Email address",
            "placeholder": "Enter your email",
            "keyboardType": "email-address",
            "required": true,
            "validation": {
              "type": "email",
              "errorMessage": "Please enter a valid email address."
            },
            "visible": true
          },
          {
            "id": "phone",
            "type": "phone",
            "label": "Mobile number",
            "placeholder": "Enter your mobile number",
            "required": true,
            "validation": {
              "type": "phone",
              "minDigits": 10,
              "errorMessage": "Please enter a valid mobile number."
            },
            "visible": true,
            "country": {
              "default": "US",
              "allowed": ["US", "GB", "IN"]
            }
          }
        ],
        "states": {
          "enter_contact": {
            "id": "enter_contact",
            "titleTextOverride": "What's your email or number?",
            "subtitleTextOverride": "You can verify your account using either your email address or your mobile number."
          },
          "verify_email": {
            "id": "verify_email",
            "title": {
              "text": "Verify your email",
              "family": "Roboto-Bold",
              "size": {
                "phone": 24,
                "tablet": 48
              },
              "weight": "700",
              "style": "normal",
              "color": {
                "light": "#1B1D26",
                "dark": "#ffffff"
              }
            },
            "body": {
              "text": [
                "We've sent a verification link to your email.",
                "Please verify your account by clicking the link sent to your email. The link expires in 2 minutes."
              ],
              "family": "Roboto-Regular",
              "size": {
                "phone": 14,
                "tablet": 20
              },
              "weight": "400",
              "style": "normal",
              "color": {
                "light": "#6B7280",
                "dark": "#98989F"
              }
            },
            "resend": {
              "text": "Send a new verification link",
              "font": {
                "family": "Roboto-Medium",
                "size": {
                  "phone": 12,
                  "tablet": 18
                },
                "weight": "500",
                "style": "normal",
                "color": {
                  "light": "#8080FF",
                  "dark": "#8080FF"
                }
              }
            }
          },
          "verify_phone": {
            "id": "verify_phone",
            "title": {
              "text": "Verify mobile number",
              "family": "Roboto-Bold",
              "size": {
                "phone": 24,
                "tablet": 48
              },
              "weight": "700",
              "style": "normal",
              "color": {
                "light": "#1B1D26",
                "dark": "#ffffff"
              }
            },
            "body": {
              "text": [
                "We sent a 6-digit code to your mobile number.",
                "Enter the code below."
              ],
              "family": "Roboto-Regular",
              "size": {
                "phone": 14,
                "tablet": 20
              },
              "weight": "400",
              "style": "normal",
              "color": {
                "light": "#6B7280",
                "dark": "#98989F"
              }
            },
            "otp": {
              "length": 6,
              "box": {
                "size": {
                  "phone": 48,
                  "tablet": 80
                },
                "fontSize": {
                  "phone": 20,
                  "tablet": 36
                },
                "fontFamily": "Roboto-Bold"
              },
              "errorMessage": "Please enter the 6-digit code."
            },
            "resend": {
              "text": "Didn't receive a code? Resend",
              "font": {
                "family": "Roboto-Medium",
                "size": {
                  "phone": 12,
                  "tablet": 18
                },
                "weight": "500",
                "style": "normal",
                "color": {
                  "light": "#8080FF",
                  "dark": "#8080FF"
                }
              }
            }
          }
        },
        "actions": [
          {
            "id": "continue",
            "type": "button",
            "label": "Continue",
            "style": "primary",
            "disabledUntilValid": true
          }
        ]
      },

      "onboarding_step2_name": {
        "id": "onboarding_step2_name",
        "type": "form",
        "theme": "dark",
        "header": {
          "title": {
            "text": "What's your name?",
            "family": "Roboto-Bold",
            "size": {
              "phone": 24,
              "tablet": 48
            },
            "weight": "700",
            "style": "normal",
            "color": {
              "light": "#1B1D26",
              "dark": "#ffffff"
            }
          },
          "secondaryTitle": {
            "text": "We'll use this to personalize your experience.",
            "family": "Roboto-Regular",
            "size": {
              "phone": 14,
              "tablet": 20
            },
            "weight": "400",
            "style": "normal",
            "color": {
              "light": "#6B7280",
              "dark": "#98989F"
            }
          }
        },
        "fields": [
          {
            "id": "first_name",
            "type": "text",
            "label": "First Name",
            "placeholder": "First Name",
            "required": true,
            "validation": {
              "minLength": 2,
              "maxLength": 8,
              "pattern": "^[a-zA-Z][a-zA-Z'.\\-’]*(?: [a-zA-Z'.\\-’]+)*$",
              "errors": {
                "required": "First name is required.",
                "minLength": "Must be at least 2 characters.",
                "maxLength": "Must be no more than 8 characters.",
                "pattern": "Only letters, hyphens (-), apostrophes ('), periods (.), and single spaces between words are allowed."
              }
            }
          },
          {
            "id": "last_name",
            "type": "text",
            "label": "Last Name",
            "placeholder": "Last Name",
            "required": false,
            "visible": true,
            "validation": {
              "minLength": 2,
              "maxLength": 8,
              "pattern": "^[a-zA-Z][a-zA-Z'.\\-’]*(?: [a-zA-Z'.\\-’]+)*$",
              "errors": {
                "required": "Last name is required.",
